<tr>
  <td><%= test.title %>
    <% if test.has_timer? %>
      <%= octicon "clock", class: "text text-warning" %>
      <%= test.timer %> min.
    <% end %>
  </td>
  <td><%= test.id %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= button_to t('.start'), start_test_path(test), class: 'btn btn-warning' %></td>
</tr>