<%= form_with model: [:admin, @category, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>
  <p>
    <%= form.label :title  %><br>
    <%= form.text_field :title %>
  </p><br>

  <p>
    <%= form.label :level  %><br>
    <%= form.text_field :level %>
  </p><br>

  <p>
    <%= form.label 'For category:' %>
    <%= form.collection_select :category_id, Category.all, :id, :title %>
  </p>

  <p><%= form.submit %></p>

  <p><%= link_to 'Back to test', admin_tests_path %></p>

<% end %>