<tr>
  <td class="align-midle"><%= test.id %></td>
  <td>
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id}  %>
    <%= render 'form_inline', test: test == @test? @test : test %>
  </td>
  <td class="align-midle"><%= test.level %></td>
  <td class="align-midle"><%= test.questions.count %></td>
  <td class="align-midle"><%= link_to t('.show'), admin_test_path(test), class: 'btn btn-light' %></td>
  <td class="align-midle">
    <%= link_to t('.edit'),
                edit_admin_test_path(test),
                class: 'btn btn-light form-inline-link',
                data: {test_id: test.id} %>
  </td>
  <td class="align-midle"><%= link_to t('.delete'),
                  admin_test_path(test),
                  method: :delete,
                  data:   { confirm: t('.are_you_sure') },
                  class: 'btn btn-light' %></td>
</tr>