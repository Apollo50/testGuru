<h2 class="mt-3"><%= t('.tests_header') %></h2>

<%= render 'shared/errors', resource: @test %>
<div class="table-responsive text-nowrap">
  <table class="table table-striped table-dark text-center">
    <thead>
      <tr>
        <th><%= t('.test_id') %></th>
        <th><%= t('.title') %></th>
        <th><%= t('.level') %></th>
        <th><%= t('.question_count') %></th>
        <th colspan="3"><%= t('.action') %></th>
      </tr>
    </thead>

    <%= render @tests %>
  </table>
</div>
<%= link_to t('.create'), new_admin_test_path, class: 'btn btn-info' %>

